<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bombom.model.MemberDAO">
	<insert id="insertMember" parameterType="com.bombom.model.MemberDTO">
		insert into bombom_member values(#{user_id}, #{user_pwd}, #{user_nickname}, #{user_name}, #{user_age}, 
			#{user_email}, #{user_gender}, #{user_addr}, #{user_phone}, #{user_profile}, 1, SYSDATE)
	</insert>
</mapper>